{
  "compilerOptions": {
    "target": "ES2017",
    "outDir": "out",
    "rootDir": "src",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "sourceMap": true,
    "moduleResolution": "node"
  },
  "exclude": [
      "node_modules"
  ]
}
